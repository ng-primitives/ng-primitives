{
  "name": "state",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/state/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/state/ng-package.json",
        "tsConfig": "packages/state/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/state/tsconfig.lib.prod.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "npm publish dist/packages/state --access public --tag latest --no-git-checks"
      }
    }
  }
}
