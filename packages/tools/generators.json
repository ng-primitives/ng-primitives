{
  "generators": {
    "primitive": {
      "factory": "./src/generators/primitive/generator",
      "schema": "./src/generators/primitive/schema.json",
      "description": "Generate a new primitive"
    },
    "directive": {
      "factory": "./src/generators/directive/generator",
      "schema": "./src/generators/directive/schema.json",
      "description": "Create a new directive"
    },
    "config": {
      "factory": "./src/generators/config/generator",
      "schema": "./src/generators/config/schema.json",
      "description": "Create a configuration provider."
    },
    "token": {
      "factory": "./src/generators/token/generator",
      "schema": "./src/generators/token/schema.json",
      "description": "Generate a token for a directive to allow it to be injected into children"
    },
    "schematic": {
      "factory": "./src/generators/schematic/generator",
      "schema": "./src/generators/schematic/schema.json",
      "description": "Create a new schematic for the primitive"
    },
    "example": {
      "factory": "./src/generators/example/generator",
      "schema": "./src/generators/example/schema.json",
      "description": "Generate an example page"
    },
    "documentation": {
      "factory": "./src/generators/documentation/generator",
      "schema": "./src/generators/documentation/schema.json",
      "description": "Add a new documentation section"
    },
    "templates": {
      "factory": "./src/generators/templates/templates",
      "schema": "./src/generators/templates/schema.json",
      "description": "Popoulate the generators with reusable component templates"
    },
    "reusable-component": {
      "factory": "./src/generators/reusable-component/reusable-component",
      "schema": "./src/generators/reusable-component/schema.json",
      "description": "Create a reusable component for a primitive."
    }
  }
}
